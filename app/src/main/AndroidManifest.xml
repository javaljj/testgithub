<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.javalj.androidapp4demo">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.ui.recycleview.RecycleViewDemoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.No" />
        <activity
            android:name=".main.ui.viewpagerfragment.ViewPagerActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.No" />
        <activity
            android:name=".main.ui.viewpagertablayout.ViewPagerTabLayoutActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.No" />
        <activity
            android:name=".main.ui.material_design.ScrollingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.No"></activity>
    </application>

</manifest>
